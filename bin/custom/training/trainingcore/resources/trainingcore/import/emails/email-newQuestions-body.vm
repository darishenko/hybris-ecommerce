<body bgcolor="#ffffff">
<table border="1">
    <thead>
    <tr>
        <th>Question</th>
        <th>Question Customer</th>
        <th>Answer</th>
        <th>Answer Customer</th>
    </tr>
    </thead>
    #foreach ($question in ${questions})
        <tr>
            <td>$question.questionCustomerName</td>
            <td>$question.question</td>
            <td>$question.answerCustomerName</td>
            <td>$question.answer</td>
        </tr>
    #end
</table>
</body>
</html>